{
  "name": "@kiichain/kiijs-rwa",
  "version": "0.2.1",
  "type": "module",
  "author": "kiiglobal.io <dev@kiiglobal.io>",
  "description": "JS SDK for kiichain rwa",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/KiiChain/kiijs-sdk",
  "license": "Apache-2.0",
  "overrides": {
    "chalk": "5.3.0",
    "strip-ansi": "7.1.0",
    "color-convert": "2.0.1",
    "color-name": "1.1.4",
    "is-core-module": "2.13.1",
    "error-ex": "1.3.2",
    "has-ansi": "5.0.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KiiChain/kiijs-sdk"
  },
  "bugs": {
    "url": "https://github.com/KiiChain/kiijs-sdk/issues"
  },
  "scripts": {
    "copy": "npx copyfiles -f ../../LICENSE README.md package.json dist",
    "clean": "npx rimraf dist",
    "prepare": "npm run build",
    "codegen": "npx ts-node scripts/codegen.ts",
    "build": "npm run clean && tsc && npm run copy",
    "build:dev": "npm run clean && npm run codegen && tsc --declarationMap && npm run copy",
    "lint": "npx eslint . --fix",
    "test": "npx jest",
    "test:ci": "npx jest",
    "test:watch": "npx jest --watch"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@cosmjs/cosmwasm-stargate": "^0.34.0",
    "@cosmjs/encoding": "^0.34.0",
    "@cosmjs/launchpad": "^0.27.1",
    "@cosmjs/proto-signing": "^0.34.0",
    "@cosmjs/stargate": "^0.34.0",
    "@cosmjs/tendermint-rpc": "^0.34.0",
    "bip39": "^3.1.0",
    "cosmjs-types": "^0.9.0"
  },
  "packageManager": "yarn@4.10.3"
}
