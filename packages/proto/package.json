{
  "name": "@kiichain/kiijs-proto",
  "version": "0.2.1",
  "author": "kiiglobal.io <dev@kiiglobal.io>",
  "type": "module",
  "description": "JS SDK for kiichain protofiles",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/KiiChain/kiijs-sdk",
  "license": "Apache-2.0",
  "overrides": {
    "chalk": "5.3.0",
    "strip-ansi": "7.1.0",
    "color-convert": "2.0.1",
    "color-name": "1.1.4",
    "is-core-module": "2.13.1",
    "error-ex": "1.3.2",
    "has-ansi": "5.0.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KiiChain/kiijs-sdk"
  },
  "bugs": {
    "url": "https://github.com/KiiChain/kiijs-sdk/issues"
  },
  "scripts": {
    "copy": "npx copyfiles -f ../../LICENSE README.md package.json dist",
    "clean": "npx rimraf dist",
    "prepare": "npm run build",
    "codegen": "npx ts-node scripts/codegen.ts",
    "build": "npm run clean && npx tsc && npx tsc -p tsconfig.declaration.json && npm run copy",
    "build:dev": "npm run clean && npm run codegen && npx tsc --declarationMap && npx tsc -p tsconfig.declaration.json && npm run copy",
    "lint": "npx eslint . --fix",
    "test": "npx jest",
    "test:ci": "npx jest",
    "test:watch": "npx jest --watch"
  },
  "devDependencies": {
    "@cosmology/telescope": "^1.10.0",
    "@types/jest": "^29.5.14",
    "dotenv": "16.4.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.1"
  },
  "dependencies": {
    "@cosmjs/amino": "^0.33.1",
    "@cosmjs/encoding": "^0.33.1",
    "@cosmjs/math": "^0.33.1",
    "@cosmjs/proto-signing": "^0.33.1",
    "@cosmjs/stargate": "^0.33.1",
    "@cosmology/lcd": "^0.14.0"
  },
  "keywords": [],
  "packageManager": "yarn@4.10.3"
}
